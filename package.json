{
  "name": "mocha-cassettes",
  "version": "1.2.2",
  "description": "Record HTTP interactions in your Mocha tests",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:bodrovis/mocha-cassettes"
  },
  "keywords": [
    "vcr",
    "mocha",
    "test",
    "record",
    "http",
    "cassettes"
  ],
  "scripts": {
    "build": "tsc",
    "test": "tsc && mocha -r ts-node/register --recursive test/**/*.spec.ts --timeout 5000"
  },
  "dependencies": {
    "mocha": "^8.2.1",
    "nock": "^13.0.6",
    "rimraf": "^3.0.2",
    "sanitize-filename": "^1.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.0",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.16.4",
    "got": "^11.8.1",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "directories": {
    "lib": "./src",
    "test": "./test"
  },
  "homepage": "https://github.com/bodrovis/mocha-cassettes#readme",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "maintainers": [
    {
      "name": "Ilya Bodrov"
    },
    {
      "name": "Roman Kutanov"
    }
  ],
  "bugs": {
    "url": "https://github.com/bodrovis/mocha-cassettes/issues",
    "email": "golosizpru@gmail.com"
  },
  "engines": {
    "node": ">= 10"
  }
}
