{
  "name": "mocha-cassettes",
  "version": "0.1.0",
  "description": "Record HTTP interactions in your Mocha tests",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:bodrovis/mocha-cassettes"
  },
  "keywords": [
    "vcr",
    "mocha",
    "test",
    "record",
    "http",
    "cassettes"
  ],
  "scripts": {
    "build": "tsc",
    "test": "tsc && mocha -r ts-node/register --recursive test/**/*.spec.ts --timeout 10000"
  },
  "dependencies": {
    "mocha": "^7.1.2",
    "nock": "^12.0.3",
    "rimraf": "^3.0.2",
    "sanitize-filename": "^1.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/request-promise": "^4.1.46",
    "@types/rimraf": "^3.0.0",
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "got": "^11.1.3",
    "ts-node": "^8.10.1",
    "typescript": "^3.1.3"
  },
  "directories": {
    "lib": "./src",
    "test": "./test"
  },
  "homepage": "https://github.com/bodrovis/mocha-cassettes#readme",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "maintainers": [
    {
      "name": "Ilya Bodrov"
    },
    {
      "name": "Roman Kutanov"
    }
  ],
  "bugs": {
    "url": "https://github.com/bodrovis/mocha-cassettes/issues",
    "email": "golosizpru@gmail.com"
  },
  "engines": {
    "node": ">= 10"
  }
}
